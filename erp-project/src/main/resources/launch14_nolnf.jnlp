      <jar href="jcalendar.jar" />
      <jar href="jnlp.jar" />
      <jar href="netscape.jar" />
      <jar href="jasperreports-1.2.7.jar" />
      <jar href="itext-1.3.1.jar" />
      <property name="SERVERURL" value="$$context/controller"/> 
   </resources>

</jnlp>

