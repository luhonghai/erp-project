<?xml version="1.0" encoding="utf-8"?>
     <!-- JNLP File for Manager Application -->
<jnlp spec="1.0+" codebase = "$$codebase">

	<application-desc main-class="org.jallinone.commons.client.ClientApplication"/>

    <information>
      <title>jAllInOne</title>
